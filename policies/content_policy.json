{
  "version": "2.0",
  "description": "Production content policy for NanoDesigner",
  "blocked_terms": [
    "explicit", "nsfw", "adult", "pornographic", "sexual", "nude", "naked",
    "violence", "violent", "weapon", "gun", "bomb", "explosive", "kill", "murder", "death", "suicide",
    "hate", "racist", "nazi", "fascist", "terrorist", "extremist", "supremacist",
    "drug", "cocaine", "heroin", "meth", "illegal", "piracy", "fraud", "scam", "phishing",
    "malware", "virus", "trojan", "backdoor", "exploit", "hack", "crack", "keylogger",
    "child", "minor", "underage", "kid", "baby", "toddler", "teen", "teenager",
    "gambling", "casino", "poker", "betting", "lottery",
    "copyright", "trademark", "stolen", "plagiarized", "unauthorized"
  ],
  "suspicious_patterns": [
    "<script[^>]*>.*?</script>",
    "javascript:",
    "data:.*base64",
    "eval\\s*\\(",
    "exec\\s*\\(",
    "\\.\\.[\\/\\\\]",
    "file:\\/\\/",
    "ftp:\\/\\/",
    "\\$\\{.*\\}",
    "<%.*%>",
    "{{.*}}",
    "\\[\\[.*\\]\\]",
    "onclick\\s*=",
    "onload\\s*=",
    "onerror\\s*=",
    "prompt\\s*\\(",
    "alert\\s*\\(",
    "confirm\\s*\\(",
    "document\\.cookie",
    "window\\.location",
    "innerHTML\\s*=",
    "outerHTML\\s*=",
    "\\bsql\\b.*\\b(select|insert|update|delete|drop|union)\\b",
    "\\b(union|select)\\b.*\\bfrom\\b",
    "\\';.*--",
    "\\/\\*.*\\*\\/",
    "\\\\x[0-9a-fA-F]{2}",
    "%[0-9a-fA-F]{2}",
    "\\\\u[0-9a-fA-F]{4}"
  ],
  "allowed_domains": [
    "openai.com",
    "api.openai.com",
    "anthropic.com",
    "api.anthropic.com",
    "huggingface.co",
    "api.huggingface.co",
    "replicate.com",
    "api.replicate.com",
    "stability.ai",
    "api.stability.ai",
    "midjourney.com",
    "discord.com",
    "github.com",
    "githubusercontent.com",
    "unsplash.com",
    "pexels.com",
    "pixabay.com",
    "freepik.com",
    "shutterstock.com",
    "gettyimages.com",
    "adobe.com",
    "figma.com",
    "canva.com",
    "dribbble.com",
    "behance.net",
    "pinterest.com",
    "instagram.com",
    "twitter.com",
    "linkedin.com",
    "youtube.com",
    "vimeo.com",
    "wikipedia.org",
    "wikimedia.org",
    "google.com",
    "googleapis.com",
    "cloudflare.com",
    "amazonaws.com",
    "azure.com",
    "vercel.app",
    "netlify.app"
  ],
  "max_content_length": 50000,
  "max_url_length": 2048,
  "max_instruction_length": 5000,
  "max_reference_count": 10,
  "allowed_file_types": [
    ".jpg", ".jpeg", ".png", ".gif", ".webp", ".svg", ".bmp", ".tiff",
    ".pdf", ".txt", ".md", ".json", ".csv", ".xml", ".yaml", ".yml",
    ".doc", ".docx", ".ppt", ".pptx", ".xls", ".xlsx",
    ".zip", ".tar", ".gz"
  ],
  "max_file_size": 10485760,
  "scan_archives": true,
  "require_mime_validation": true,
  "block_executable_extensions": [
    ".exe", ".bat", ".cmd", ".com", ".scr", ".pif", ".msi", ".dll",
    ".sh", ".bash", ".zsh", ".fish", ".ps1", ".vbs", ".js", ".jar",
    ".app", ".dmg", ".pkg", ".deb", ".rpm", ".apk", ".ipa"
  ],
  "content_filters": {
    "enable_profanity_filter": true,
    "enable_pii_detection": true,
    "enable_malware_scan": true,
    "enable_phishing_detection": true,
    "enable_spam_detection": true
  },
  "rate_limits": {
    "requests_per_minute": 60,
    "requests_per_hour": 1000,
    "requests_per_day": 10000,
    "file_uploads_per_hour": 100
  },
  "logging": {
    "log_blocked_content": true,
    "log_suspicious_content": true,
    "log_security_events": true,
    "retention_days": 90
  }
}
